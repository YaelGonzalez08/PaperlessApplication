<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/primaryColor"/>
            <corners android:radius="@dimen/dimen_20" />
            <stroke
                android:width="@dimen/dimen_1"
                android:color="@color/gray_text"/>
            <size android:height="@dimen/dimen_16" />
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_gray_white_background_checklist"/>
            <corners android:radius="@dimen/dimen_28" />
            <stroke
                android:width="@dimen/dimen_1"
                android:color="@color/gray_text"/>
            <size android:height="@dimen/dimen_16" />
        </shape>
    </item>
</selector>